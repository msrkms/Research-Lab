@model IEnumerable<Research_Lab.Models.AppUser>

@{
    ViewData["Title"] = "ShowAllLabAssistant";
    Layout = "~/Views/Shared/_LayoutAuthorizedamin.cshtml";
}


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
           
            <td>
                <a asp-action="Edit" asp-route-id="@item.AppUserId">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.AppUserId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
